{
  "name": "@ax/uilib",
  "version": "1.0.0",
  "description": "A reusable React UI component library",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vite build --mode library && npm run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build:lib": "vite build --mode library",
    "dev": "vite build --watch --mode library",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "library"
  ],
  "author": "Hidenori Takaku <hidenori@aquilaxeno.com>",
  "company": "Aquila Xeno",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^3.9.1"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.4.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.468.0",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18"
  }
}
